        """ ;
            sh:message """
        rr:constant must be an IRI.
        """ ;
            sh:name "rr:constant" ;
            sh:nodeKind sh:IRI ;
            sh:path <http://www.w3.org/ns/r2rml#constant> ],
        [ sh:datatype xsd:string ;
            sh:description """
