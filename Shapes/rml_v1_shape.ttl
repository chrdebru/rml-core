            sh:path [ sh:alternativePath ( <http://www.w3.org/ns/r2rml#template> <http://www.w3.org/ns/r2rml#column> <http://www.w3.org/ns/r2rml#constant> <http://semweb.mmlab.be/ns/rml#reference> ) ] ],
        [ sh:datatype xsd:string ;
